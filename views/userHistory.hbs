<h2>{{session.user.userName}} {{session.user.email}} History</h2>

<div class="row">
    <div class="col-md-12">
        <div class="table-responsive">
            <table class="table">
                <tr>
                    <td><b>Login Date/Time</b></td>
                    <td><b>Client Information</b></td>
                </tr>
                {{#each session.user.loginHistory}}
                    <tr>
                        <td>{{dateTime}}</td>
                        <td>{{userAgent}}</td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </div>
</div>